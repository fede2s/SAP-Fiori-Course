<mvc:View controllerName = "fiori_ini.SAPUI5.controller.App"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    displayBlock="true">
    <Page title="{i18n>appTitle}">
        <headerContent>
            <Button id="helloDialogHeader"
                icon="sap-icon://action"
                press=".onOpenDialogHeader"
                class="sapUiSmallMarginEnd"/>
        </headerContent>
        <content>
            <mvc:XMLView viewName="fiori_ini.SAPUI5.view.HelloPanel"/>
            <mvc:XMLView viewName="fiori_ini.SAPUI5.view.InvoicesList"/>
        </content>
    </Page>
</mvc:View>